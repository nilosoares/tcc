#!/bin/bash

if [[ -z "$1" ]] ; then
    echo "Por favor, informe o número da consulta (1, 8, 15, 20, 21 ou 22)."
    exit 1
fi

if [ ! -f "resources/tpc-h-mongo/queries/Q$1.js" ]; then
    echo "Por favor, informe uma consulta válida (1, 8, 15, 20, 21 ou 22)."
    exit 1
fi

if [ ! -f "resources/tpc-h-mongo/queries/Q$1.out" ]; then
    echo "O log desta consulta não existe."
    exit 1
fi

# Print the query log
cat ./resources/tpc-h-mongo/queries/Q$1.out;